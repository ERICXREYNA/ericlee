#include<stdio.h>
int main (void) {
     printf(" hello Eric "); 
     printf(" my height is %d m %d cm \n",1,64);
     printf(" this building is %5d cm \n", 999);
     printf(" this building is %5d cm \n", 4560);
     printf(" this building is %5d cm \n", 55660);
     printf(" this building is %5d cm \n", 668590);
     printf(" this building is %5d cm \n", 1234567890);
     printf(" pi = %f \n ", 3.1415926535897932384626433832795);
}
