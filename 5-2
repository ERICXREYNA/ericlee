#include <stdio.h>

int main()
{
    int a;
    printf(" please input a \n");
    scanf(" %d" ,&a);
    if(a > 8   )
        printf("hello a > 8");
    else if(a==1)
        printf("Monday");
    else if(a==2)
        printf("Tuesday");  
    else if(a==3)
        printf("Wednesday");
     else if(a==4)    
        printf("Thersday");  
     else if(a==5)      
         printf("Friday");
     else if(a==6)      
         printf("Saturday");
    else if(a==7)      
         printf("Sunday"); 
     
     
     
     return 0;
