int main() {
    int a, b, product;
    a = 20;
    b = 7;

 
    // Calculating product
    product = a + b;

    printf("product = %d", product);
    
     product = a - b;

    printf("product = %d", product);
    
     product = a % b;

    printf("Product = %d", product);

    product = a / b;

    printf("Product = %d", product);
    
    return 0;
}
